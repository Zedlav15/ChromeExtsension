{
    "manifest_version": 2,
    "name": "Far fay extension",
    "version": "1.0",
    "description": "This is the Far Fay Group Chrome extension",
    "permissions": [
      "storage",
      "tabs",
      "http://localhost:3000/*",
      "https://www.facebook.com/*"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "Logo2.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "Logo2.png"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'"
  }
  